*.font:     mononoki NF:pixelsize=18:antialias=true:autohint=true
*.alpha:    1.00

!!! urxvt

URxvt.font: xft:mononoki NF:pixelsize=18:antialias=true:autohint=true
URxvt.scrollBar: false
URxvt.internalBorder: 12
URxvt.saveLines: 2048
URxvt.geometry: 92x24
URxvt.cursorBlink: true
URxvt.cursorUnderline: false
URxvt.letterSpace: 0
URxvt.lineSpace: 0
URxvt.iso14755: false
URxvt.iso14755_52: false
URxvt.perl-ext-common: default,matcher,font-size,selection-to-clipboard
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.clipboard.autocopy: true
URxvt.matcher.button: 1
URxvt.matcher.rend.0: Uline Bold fg5

!!! sxiv

Sxiv.font: mononoki NF:size=14
Sxiv.foreground: #d1d1d1
Sxiv.background: #151515

