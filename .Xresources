*.font:     mononoki NF:pixelsize=18:antialias=true:autohint=true
*.alpha:    1.00

!!! urxvt

URxvt.font: -*-terminus-*-*-*-*-*-140-*-*-*-*-*-*,xft:terminus:pixelsize:12
URxvt.boldFont: -*-terminus-*-*-*-*-*-140-*-*-*-*-*-*,xft:terminus:pixelsize:12
URxvt.italicFont: -*-terminus-*-*-*-*-*-140-*-*-*-*-*-*,xft:terminus:pixelsize:12
URxvt.scrollBar: true
URxvt*scrollBar_right: true
URxvt*scrollBar_floating: true
URxvt.internalBorder: 12
URxvt.saveLines: 2048
URxvt.geometry: 120x40
URxvt.cursorBlink: true
URxvt.cursorUnderline: false
URxvt.resize.increment: 1
URxvt.letterSpace: 1
URxvt.fading: 30
URxvt.lineSpace: 3
URxvt.transparent: false
URxvt.depth: 32
URxvt.scrollstyle: plain
URxvt.hold: false
URxvt.iso14755: false
URxvt.iso14755_52: false
URxvt.underlineColor: red
URxvt.scrollTtyOutput: true
URxvt.scrollTTyKeypress: true
Urxvt.termName: rxvt
URxvt.keysym.M-Up:     command:\033]720;1\007
URxvt.keysym.M-Down:   command:\033]721;1\007
URxvt*perl-ext-common:
URxvt*perl-ext:

Xft.antialias: true
Xft.hinting: true
Xft.hintstyle: hintslight

